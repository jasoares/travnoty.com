%h3= "A new round of #{@server.host} is starting on #{l @round.start_date}"

%h4= raw "Server: #{link_to @server.host, @server.url}"
%table
  %thead
    %tr
      %th Name
      %th Code
      %th Host
      %th Speed
      %th World ID
  %tbody
    %tr
      %td= @server.name
      %td= @server.code
      %td= @server.host
      %td= @server.speed
      %td= @server.world_id
